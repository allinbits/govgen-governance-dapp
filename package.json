{
  "name": "govgen-governance",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "lint": "eslint -c .eslintrc.cjs \"./{tests,src}/**/*.{ts,js,tsx,vue}\" --no-error-on-unmatched-pattern --ignore-path .gitignore",
    "lint:fix": "eslint -c .eslintrc.cjs \"./{tests,src}/**/*.{ts,js,tsx,vue}\" --fix --no-error-on-unmatched-pattern --ignore-path .gitignore",
    "preview": "vite preview",
    "test:e2e": "npx playwright install && playwright test",
    "test:unit": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "vue": "^3.4.15"
  },
  "devDependencies": {
    "@playwright/test": "^1.41.2",
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitest/coverage-v8": "^1.2.2",
    "@vitest/ui": "^1.2.2",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.21.1",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vitest": "^1.2.2",
    "vue-tsc": "^1.8.27"
  }
}
